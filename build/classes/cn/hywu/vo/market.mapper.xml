<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hywu.vo.market.mapper">
	<select id="listmarketchina" resultType="Market">
		select * from market where place="china"
	</select>
	<select id="listmarketothers" resultType="Market">
		select * from market where place!="china"
	</select>
</mapper>